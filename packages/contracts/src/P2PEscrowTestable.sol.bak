pragma solidity ^0.8.20;

import "./P2PEscrow.sol";

contract P2PEscrowTestable is P2PEscrow {
    address public TEST_TOKEN;

    constructor(address _backendSigner, address _testToken)
        P2PEscrow(_backendSigner)
    {
        TEST_TOKEN = _testToken;
    }

    function _token() internal view override returns (address) {
        return TEST_TOKEN;
    }
}

